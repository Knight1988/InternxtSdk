{
  "name": "@dragon-knight/internxt-sdk",
  "version": "0.1.1",
  "description": "Complete Internxt SDK wrapper with login, file operations, and folder management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Knight1988/InternxtSdk.git"
  },
  "bugs": {
    "url": "https://github.com/Knight1988/InternxtSdk/issues"
  },
  "homepage": "https://github.com/Knight1988/InternxtSdk#readme",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "start": "npm run build && node example.js",
    "demo": "npm run build && node example.js",
    "dev": "ts-node src/index.ts",
    "test": "mocha",
    "test:unit": "mocha --config .mocharc.unit.json",
    "test:integration": "mocha 'test/integration/**/*.test.ts'",
    "test:watch": "mocha --watch",
    "test:coverage": "echo 'Coverage not configured yet. Install nyc or c8 for coverage.'"
  },
  "keywords": [
    "internxt",
    "sdk",
    "login",
    "authentication",
    "cloud-storage",
    "file-upload",
    "file-download",
    "encryption"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@internxt/cli": "^1.5.6",
    "@internxt/inxt-js": "^2.2.9",
    "@internxt/lib": "^1.3.1",
    "@internxt/sdk": "^1.11.12",
    "dotenv": "^16.4.5",
    "openpgp": "^6.2.2"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.7.1",
    "@types/sinon": "^17.0.4",
    "chai": "^6.2.0",
    "mocha": "^11.7.4",
    "otplib": "^12.0.1",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
